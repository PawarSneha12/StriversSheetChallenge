//Ques.12 Missing and repeating Number


//Solution---

public class Solution {

    public static int[] missingAndRepeating(ArrayList<Integer> arr, int n) {
        int[]res=new int[2];
        int[]fre=new int[n+1];
        for(int i=0;i<n;i++){
            fre[arr.get(i)]++;
        }
        for(int i=1;i<=arr.size();i++){
            if(fre[i]==0){
                res[0]=i;
            }
            else if(fre[i]>1){
                res[1]=i;
            }
        }
        return res;
    }
}
